{
  "name": "@rhkdguskim/vicscoreclientlib",
  "version": "1.0.6",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "dts": true,
    "clean": true,
    "treeshake": true,
    "minify": false,
    "skipNodeModulesBundle": true
  },
  "keywords": [],
  "author": "rhkdguskim",
  "license": "ISC",
  "description": "VICS core client library",
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "ts-proto": "^2.7.7",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "long": "^5.3.2",
    "protobufjs": "^7.5.3",
    "react-uuid": "^2.0.0"
  }
}
