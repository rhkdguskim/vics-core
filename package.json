{
  "name": "@rhkdguskim/vicscoreclientlib",
  "version": "1.0.17",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && mkdir -p dist/util dist/worker dist/controller dist/parsers dist/remuxer && cp -R src/util/*.js dist/util/ 2>/dev/null || true && cp -R src/controller/*.js dist/controller/ 2>/dev/null || true && cp -R src/parsers/*.js dist/parsers/ 2>/dev/null || true && cp -R src/remuxer/*.js dist/remuxer/ 2>/dev/null || true && cp src/H264Decoder.js dist/ 2>/dev/null || true && cp src/worker/H264DecoderWorker.js dist/worker/ 2>/dev/null || true",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "dts": true,
    "clean": true,
    "treeshake": true,
    "minify": false,
    "bundle": true,
    "splitting": false,
    "skipNodeModulesBundle": true
  },
  "keywords": [],
  "author": "rhkdguskim",
  "license": "ISC",
  "description": "VICS core client library",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "ts-proto": "^2.7.7",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "long": "^5.3.2",
    "moment": "^2.30.1",
    "protobufjs": "^7.5.3",
    "react-uuid": "^2.0.0"
  }
}
